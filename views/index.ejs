    <div class="todo-app" id="draggable"><p class="todo-app-title"><img src="/public/images/app-titles/todo-title.png"> </p><div class="todo-container">
        <form id="todoForm">
            <input type="text" id="todoInput">
            <button type="button" onclick="todoList()">Add Item</button>
          </form>
          <ul id="todoList"></ul>
      </div></div>
   

    <div class="notes-app" id="draggable"><p class="notes-app-title"><img src="/public/images/app-titles/notes-title.png"></p><div class="notes-container">
      <form id="todoForm">
      <input type="text" id="noteInput">
      <button type="button" onclick="noteList()">Add Note</button>
    </form>
    <ul id="noteList"></ul></div></div>

    <div class="player-app" id="draggable"><p class="player-app-title"><img src="/public/images/app-titles/player-title.png"></p><div class="player-container">
      <input type="image" src="public\images\player-icons\university.png" class="player-icons">
      <input type="image" src="public\images\player-icons\coffee-shop.png" class="player-icons">
      <input type="image" src="public\images\player-icons\farm.png" class="player-icons">
      <input type="image" src="public\images\player-icons\storm.png" class="player-icons-right">
      <input type="image" src="public\images\player-icons\park.png" onclick="playAudio()" class="player-icons-right">

        <audio id="myAudio">>
            <source src="https://freesound.org/people/inchadney/sounds/61697/download/61697__inchadney__park-in-autumn.wav" type="audio/mpeg">
          </audio>
      
    </div>
    <div class="player-app-controls">
      <input type="image" src="public\images\player-icons\play-pause.png" onclick="togglePlay()" class="player-control-buttons">
      <input type="image" src="public\images\player-icons\stop.png" onclick="stopAudio()" class="player-control-buttons"> 
      <input type="range" id="player-volume-slider" onchange="changeVolume()" step="0.05" min="0" max="1" value="1">
    </div></div>

    <div class="pomodoro-app" id="draggable"><p class="pomodoro-app-title"><img src="/public/images/app-titles/pomodoro-title.png"></p>    <div class="container">
        <br><br>
            <div class="timer-container">
              <button id="session-plus">
                <i class="fa fa-chevron-up fa-3x"></i>
              </button>
              <div id="timer">25:00</div>
              <button id="session-minus">
                <i class="fa fa-chevron-down fa-3x"></i>
              </button>
            </div>
            <br>
            <div class="btns">
              <button class="green" id="start-btn">START</button>
              <button class="blue" id="reset-btn">RESET</button>
              <button class="red" id="stop-btn">STOP</button>

            </div>
